{
    "name": "Tabs2Links",
    "version": "<%= htmlWebpackPlugin.options.appversion %>",
    "description": "<%= htmlWebpackPlugin.options.appdescription %>",
    "icons": {
        "128": "assets/icon_128.png",
        "48": "assets/icon_48.png",
        "16": "assets/icon_16.png"
    },
    "browser_action": {
        "default_title": "Tabs2Links",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "manifest_version": 2
}
